{
  "name": "외대 종강시계",
  "description": "HUFS-Clock : 이 시계는 한국외대 학생들에게 종강이 몇 일 남았는지 보여줍니다. 종강 화이팅!",
  "version": "2.0.0",
  "manifest_version": 3,
  "icons": {
    "48": "assets/icons/icon_48.png",
    "128": "assets/icons/icon_128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "PAGE_TO_OVERRIDE": "index.html"
  },
  "permissions": ["background", "storage", "tabs"],
  "host_permissions": [
    "https://us-central1-hufssemesterclockfirebase.cloudfunctions.net/"
  ]
}
